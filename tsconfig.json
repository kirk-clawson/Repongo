{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",

        "noImplicitAny": false,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,

        "sourceMap": true
    }
}